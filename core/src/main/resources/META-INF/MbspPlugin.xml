<?xml version="1.0" encoding="UTF-8"?>
<idea-plugin>

    <extensions defaultExtensionNs="com.intellij">
        <fileTypeFactory implementation="tk.cofe.plugin.mbsp.MbspFileTypeFactory"/>

        <lang.parserDefinition language="MBSP" implementationClass="tk.cofe.plugin.mbsp.MbspParserDefinition"/>

        <multiHostInjector order="first" implementation="tk.cofe.plugin.mbsp.MbspInject"/>

        <completion.contributor id="mybatis-sql-param-completion" language="MBSP" order="first"
                                implementationClass="tk.cofe.plugin.mybatis.completion.MbspFileSqlParameterCompletionContributor"/>
    </extensions>

</idea-plugin>